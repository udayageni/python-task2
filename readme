# python_task2
#sorting
def sort(nums):
  for i in range(len(nums)-1,0,-1):
    for j in range(i):
      if nums[j]>nums[j+1]:
        temp=nums[j]
        nums[j]=nums[j+1]
        nums[j+1]=temp
nums=[3,6,8,2,5,9]
sort(nums)
print(nums)
#other way of sorting
numbers=[7,9,5,3,1,6]
numbers.sort()
numbers.sort(reverse=True)
print(numbers)
